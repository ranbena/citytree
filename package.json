{
  "name": "citytree",
  "description": "CityTree's landing page",
  "version": "1.0.0",
  "author": "Ran Byron <ranbena@gmail.com>",
  "license": "MIT",
  "keywords": [
    "citytree"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "bundle": "gatsby clean && gatsby build && gatsby serve",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "bootstrap": "^4.5.0",
    "gatsby": "2.25.0",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-htaccess": "^1.4.0",
    "gatsby-plugin-intl": "git://github.com/ranbena/gatsby-plugin-intl.git#standalone",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sass": "^2.3.2",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "node-sass": "^7.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0",
    "react-parallax": "^3.0.3",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-root-import": "^6.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-root-import": "^1.0.4",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3"
  }
}
